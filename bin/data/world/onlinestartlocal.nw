function onPlayerEnters(player) {
  // Initialize the this. variables
  npc.timeout = 0.05;
  walkmode = 1;
  //if (wasthrown && walkmode>1) walkmode = 1;

  npc.ani = 'idle';
}

function vecx(dir) {
	return Math.cos(((Math.PI / 2) * dir+1));
}

function vecy(dir) {
	return Math.sin(((Math.PI / 2) * dir+1));
}

function random(min, max) {
 	return Math.floor(Math.random() * (max - min) ) + min;
}

// Walking stuff
function onTimeout () {
	// WALKING
	npc.ani = 'walk';
	let newx = npc.x + vecx(npc.dir)*speed;
	let newy = npc.y - vecy(npc.dir)*speed;
	runcounter--;
	if (walkmode==1) { // RANDOM
		if ( runcounter > 0 ) {
			let testx = newx + 1.5 + vecx(npc.dir);
			let testy = newy - 2 + vecy(npc.dir);

			if (npc.level.onwall(testx,testy)) {
				npc.dir = (npc.dir+2)%4;
				runcounter = random(0,60);
			} else {

				npc.x = newx;
				npc.y = newy;
			}
		} else {
			runcounter = random(10,40);
			npc.dir = ((npc.dir+1+random(0,2)*2)%4);
		}
	}

	if (walkmode > 1 && runcounter <= 0)
		runcounter = 20;

	npc.message('players: ' + server.players.length + server.players[random(0,1)].account);
	npc.timeout = 0.05;
}

